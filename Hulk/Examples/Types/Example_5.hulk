type Point(x, y) {
    x = x;
    y = y;

    // ... Se añadio lo de debajo para que funcione

    getX() => self.x;
    getY() => self.y;

    setX(x) => self.x := x;
    setY(y) => self.y := y;
}
// Se añadio lo anterior para que funcione






let pt = new Point(3,4) in
    print("x: " @ pt.getX() @ "; y: " @ pt.getY());
