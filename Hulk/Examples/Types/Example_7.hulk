type Point(x, y) {
    x = x;
    y = y;

    // ... Se añadio lo de debajo para que funcione

    getX() => self.x;
    getY() => self.y;

    setX(x) => self.x := x;
    setY(y) => self.y := y;
}
// Se añadio lo anterior para que funcione



type PolarPoint inherits Point {
    rho() => sqrt(self.getX() ^ 2 + self.getY() ^ 2);
    // ...
}

let pt = new PolarPoint(3,4) in
    print("rho: " @ pt.rho());